<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width">

    <meta http-equiv="cache-control" content="max-age=0" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
    <meta http-equiv="pragma" content="no-cache" />

    <title>Friends Tracker</title>

    <link rel="icon" href="@Url.Content("~/content/favicon.ico")" type="image/x-icon" />
    <link rel="shortcut icon" href="@Url.Content("~/content/favicon.ico")" type="image/x-icon" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div id="container">
        @RenderBody()
    </div>

    <script type="text/javascript" src="~/scripts/require.js"></script>
    <script type="text/javascript">
        require(['/scripts/modules/main.js'], function (main) {
            main("@ViewBag.PageCodename");
        })
    </script>

    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    @RenderSection("googleMapSourceScript", required: false)
    @RenderSection("mapInitializationSrcipts", required: false)*@
</body>
</html>
